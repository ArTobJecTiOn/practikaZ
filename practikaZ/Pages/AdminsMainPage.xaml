<Page x:Class="practikaZ.Pages.AdminsMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:practikaZ.Pages"
      mc:Ignorable="d" d:DesignWidth="1000" MinHeight="500" MinWidth="1000"
      Title="AdminsMainPage" Background="White" Height="753">

    <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="0,0,0,-189">



        <StackPanel Grid.Row="1" Orientation="Vertical" HorizontalAlignment = "Left">
            <Button x:Name="Profile" Width="140" Content="Профиль" Height="40" Click="Profile_Click" Margin="10"/>
            <Button x:Name="Achivement" Width="140" Content="Достижения" Height="40" Click="Achivement_Click" Margin="10"/>
            <Button x:Name="AddAchieve" Width="140" Content="Добавить достижение" Height="40" Click="AddAchieve_Click" Margin="10"/>
            <Button x:Name="CreateAchive" Width="140" Content="Создать достижение" Height="40" Click="CreateAchive_Click" Margin="10"/>
            <Button x:Name="Announcement" Width="140" Content="Сообщения" Height="40" Click="Announcement_Click" Margin="10"/>
            <Button x:Name="CreateAnnounce" Width="140" Content="Создать сообщение" Height="40" Click="CreateAnnounce_Click" Margin="10"/>
            <Button x:Name="Order" Width="140" Content="Заказы оборудования" Height="40" Click="Order_Click" Margin="10"/>
            <Button x:Name="StaffList" Width="140" Content="Список сотрудников" Height="40" Click="StaffList_Click" Margin="10"/>
            <Button x:Name="RegUser" Width="140" Content="Зарегистрировать пользователя" Height="40" Click="RegUser_Click" Margin="10"/>
        </StackPanel>



    </ScrollViewer>

</Page>
